batch 2500                                                      ;number of traces to analyze at each iteration
alpha 0.05                                                       ;confidence interval of the test
samplingFreq 500                                                ;sampling frequency of the oscilloscope [Mhz]
clockFreq 120                                                   ;frequency of the processor [Mhz]
startSample 0                                                  ;starting sample
endSample 0                                                  ;ending sample. 0 means to the end
imageParams {
    displayGrid false                                               ;true if you want a grid that shows the clock
    xtics 0                                                        ;tics on x axis
    imageWidth 2000                                             ;width of the output image
    imageHeight 850                                             ;height of the output image
}
order 1                                                         ;order of the test
maxTau2 50                                                      ;max tau to consider for order 2
