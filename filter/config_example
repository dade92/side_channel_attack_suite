outputFilename      sin_filtered.dat                                    ;name of the file in output
step                2                    ;number of traces to put in memory
samplingFreq        1000                     ;sampling frequency
padding             zero                    ;padding type
filterCombination doNothing                 ;what to do when more windows are combined: doNothing,clamp,normalize
;filterFile aes_adaptive_filter.dat          ;the file of the adaptive filter
;list of filter windows
windows {   
    first {
        filterType bandPass     ;type of filter:low pass, band pass or high pass
        windowType rect         ;type of window: rect,
        lowFrequency 0		;high frequency
	highFrequency 30
    }
}

